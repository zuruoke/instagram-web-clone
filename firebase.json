{
  "hosting": {
    "public": "build/web",
    "predeploy": [
      "flutter clean",
      "flutter build web"
    ], 
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
